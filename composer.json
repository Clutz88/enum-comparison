{
    "name": "clutz88/enum-comparison",
    "description": "A package for adding comparison functions to enum classes",
    "keywords": [
        "Clutz88",
        "enum-comparison"
    ],
    "homepage": "https://github.com/clutz88/enum-comparison",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Cutts",
            "email": "github@chriscutts.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Clutz88\\EnumComparison\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clutz88\\EnumComparison\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
